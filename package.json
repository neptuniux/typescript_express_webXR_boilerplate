  "description": "The is a demo about threejs webxr and typescript",
  "main": "server.js",
  "scripts": {
    "build": "npx tsc && npm run copy_public_windows",
    "start": "npm run clean_dist && npm run build && npm run webpack && node dist/server.js",
    "dev": "npx nodemon --exec npm run start",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean_dist": "rimraf dist",
    "copy_public_windows": "xcopy src\\public\\css dist\\public\\css /i /y /q /s && xcopy src\\public\\images dist\\public\\images /i /y /q /s && xcopy src\\views\\*.ejs dist\\views /s /i /y /q",
    "webpack": "webpack"
  },
